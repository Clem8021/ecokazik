<%= form_with model: [:admin, @teacher] do |f| %>
  <%= f.label :first_name %>
  <%= f.text_field :first_name %>

  <%= f.label :last_name %>
  <%= f.text_field :last_name %>

  <%= f.label :instrument %>
  <%= f.text_field :instrument %>

  <%= f.label :bio %>
  <%= f.text_area :bio %>

  <%= f.label :photo_url %>
  <%= f.text_field :photo_url %>

  <%= f.submit %>
<% end %>
